      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1",
      "id": "betweena-backend"
    },
    "rewrites": [
      {
        "source": "**",
        "apphosting": "betweena-backend"
      }
    ]
  }
}
